{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": false,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 80,
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "ignore",
  "singleAttributePerLine": true,
  "quoteProps": "preserve",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^(node:)?path$",
    "^(node:)?vite$",
    "^vue$",
    "^vue",
    "^@vitejs/plugin-vue$",
    "^vite-plugin-vue-devtools$",
    "^vite-plugin-pages$",
    "^vite-plugin-vue-layouts$",
    "^pinia",
    "^unplugin-vue-components/vite$",
    "^@tailwindcss/vite$",
    "^@primevue/auto-import-resolver$",
    "^primevue/",
    "^@primeuix/",
    "^vitest",
    "^lodash",
    "^@shared/(?!__)",
    "^@ui/(?!__)",
    "^@shared/__tests__/",
    "^@shared/__tests__/__mocks__/",
    "^@ui/__tests__/",
    "^@/",
    "^\\./"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
